<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appproyecto">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ObservacionModificada" />
        <activity android:name=".activities.ModificarObservacion" />
        <activity
            android:name=".activities.BorrarObservacion"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.ObservacionesActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.AcercaDe"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.DetalleObservacion" />
        <activity
            android:name=".activities.AgregarObservacion"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.Telefono"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.MainActivity">

        </activity>
    </application>

</manifest>